{
	"name": "@young-money/test-pkg",
	"version": "1.0.0",
	"description": "A tiny npm package that provides a simple add(a, b) function.",
	"keywords": ["add", "sum", "utility"],
	"license": "MIT",
	"author": "",
	"type": "commonjs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": ["dist"],
	"sideEffects": false,
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"test": "jest",
		"test:watch": "jest --watchAll",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"prepublishOnly": "npm run lint && npm run format:check && npm run test && npm run build"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.5",
		"typescript": "^5.6.3",
		"@types/jest": "^29.5.12"
	}
}
