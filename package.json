{
	"name": "@young-money/test-pkg",
	"version": "1.0.0",
	"description": "A tiny npm package that provides a simple add(a, b) function.",
	"keywords": ["add", "sum", "utility"],
	"license": "MIT",
	"author": "",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": ["dist"],
	"sideEffects": false,
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "jest",
		"test:watch": "jest --watchAll",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"prepublishOnly": "npm run lint && npm run format:check && npm run test && npm run build"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@swc/jest": "^0.2.36",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"ts-jest": "^29.2.5",
		"tsup": "^8.1.0",
		"typescript": "^5.6.3",
		"@types/jest": "^30.0.0"
	}
}
